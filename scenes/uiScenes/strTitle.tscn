func _ready() -> void:
	self.visible=true
	var _v = global.connect(\"photoPlaced\",self,'goAway')

func goAway() -> void:
	var _v = $tween.interpolate_property(self,'modulate:a',1,0,0.3,Tween.TRANS_QUINT,Tween.EASE_IN)
	_v = $tween.start()
	yield($tween,\"tween_all_completed\")
	self.queue_free()
"

